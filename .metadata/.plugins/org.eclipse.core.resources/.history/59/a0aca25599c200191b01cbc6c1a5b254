package list;

import java.awt.List;
import java.util.ArrayList;
import java.util.Collection;
import java.util.HashMap;

import java.util.Set;
import java.util.TreeSet;

public class Sdetail {
	private int id;
	private String name;
	private String adress;
	private static int option;
	Set <Sdetail>s = new TreeSet<Sdetail>();

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAdress() {
		return adress;
	}

	public void setAdress(String adress) {
		this.adress = adress;
	}

	public Sdetail() {
		super();
	}

	public Sdetail(int id, String name, String adress) {
		super();
		this.id = id;
		this.name = name;
		this.adress = adress;
	}

	public void addDetail(int id, String name, String address) {
		s.add(new Sdetail(id,name,address));
	}

	@Override
	public String toString() {
		return "Sdetail [id=" + id + ", name=" + name + ", adress=" + adress + ", s=" + s + "]";
	}

	public void viewDetail() {
		for (Sdetail s : s) {

			System.out.println(s);
		}
	}

}
