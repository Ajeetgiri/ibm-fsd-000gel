package thread;

public class MyThreadDemo implements Runnable {
	public static void main(String[] args) {
		MyThreadDemo thread = new MyThreadDemo();
		Thread t1 = new Thread(thread);
		Thread t2 = new Thread(thread);
		t1.start();
		t2.start();
	
	}
	@Override
	public void run() {
		for (int i=0;i<20;i++) {
			System.out.println("==>:"+i);
		}
		
	}

}
