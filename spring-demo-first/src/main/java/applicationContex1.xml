<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean name="theEmployee" class="com.bean.Employee" >
<constructor-arg name="id" value="1"></constructor-arg>
<constructor-arg name="firstName" value="Ajeet"></constructor-arg>
<constructor-arg name="lastName" value="Giri"></constructor-arg>
<constructor-arg name="email" value="ajeetgiri.giri@gmail.com"></constructor-arg>
</bean>
</beans>
